<UserControl x:Class="ROS_ImageWPF.RobotControl"
                      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                      Loaded="UserControl_Loaded" SnapsToDevicePixels="True" SizeChanged="UserControl_SizeChanged"
                      xmlns:c="clr-namespace:ROS_ImageWPF">
 <!--     <UserControl.Resources>
         <c:RobotControl x:Key="myDataSource"/> 
    </UserControl.Resources>  -->
    <!-- <UserControl.DataContext>
        <Binding Source="{StaticResource myDataSource}"/>
    </UserControl.DataContext> -->
    <Grid SnapsToDevicePixels="True" Width="1680" Height="1050" >
        <Canvas Name="RobotCanvas" Width="auto" Height="auto" >

            <Ellipse Name="robot" x:FieldModifier="public" SnapsToDevicePixels="True" Width="14" Height="14" Fill="Blue" />
            <Rectangle Name="goal" x:FieldModifier="public" SnapsToDevicePixels="True" Width="14" Height="14" Fill="Red" />
        </Canvas>
    </Grid>
</UserControl>