<UserControl x:Class="ROS_IMUUtil.Gimbal"
                      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                      Loaded="UserControl_Loaded" SnapsToDevicePixels="True" SizeChanged="UserControl_SizeChanged" MinWidth="200" MinHeight="200">
  <Grid SnapsToDevicePixels="True">
    <Rectangle SnapsToDevicePixels="True" Stretch="Fill">
      <Rectangle.Fill>
        <ImageBrush x:Name="image" />
      </Rectangle.Fill>
    </Rectangle>
  </Grid>
</UserControl>